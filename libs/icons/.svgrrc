{
  "outDir": "src/lib",
  "index": true,
  "memo": true,
  "typescript": true,
  "svgoConfig": {
    "multipass": true,
    "js2svg": {
      "indent": 2,
      "pretty": true
    },
    "plugins": [
      "preset-default",
      "prefixIds",
      {
        "name": "sortAttrs",
        "params": {
          "xmlnsOrder": "alphabetical"
        }
      }
    ]
  }
}
